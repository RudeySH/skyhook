{
  "name": "skyhook",
  "version": "0.0.1",
  "description": "Parses webhooks and forwards them in the proper format to Discord.",
  "scripts": {
    "tsc": "tsc",
    "build": "npm run tsc",
    "gcp-build": "npm run build",
    "start": "node dist/index.js",
    "buildstart": "npm run build && npm run start",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "lint": "eslint . --ext .js,.ts --fix",
    "deploy": "npm run build && gcloud app deploy",
    "logs": "gcloud app logs tail -s default",
    "dev": "nodemon"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/gravatar": "^1.8.3",
    "@types/luxon": "^2.3.2",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.26",
    "@types/turndown": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.12.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@sentry/node": "^6.19.7",
    "axios": "^0.27.2",
    "body-parser": "^1.19.2",
    "camelcase": "^6.3.0",
    "cors": "^2.8.5",
    "dotenv": "16.0.0",
    "express": "^4.18.1",
    "gravatar": "^1.8.2",
    "luxon": "^2.3.1",
    "turndown": "^7.1.1",
    "winston": "^3.6.0"
  },
  "engines": {
    "node": "16.x.x",
    "npm": "8.x.x"
  },
  "repository": {
    "url": "https://github.com/Commit451/skyhook"
  },
  "bugs": {
    "url": "https://github.com/Commit451/skyhook/issues"
